{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "> 1%",
            "last 3 versions",
            "ie > 8",
            "chrome >= 37"
          ]
        }
      }
    ],
    "stage-0"
  ],
  "plugins": [
    "lodash",
    "syntax-dynamic-import",
    "transform-runtime",
    ["component", [
      {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-chalk"
      },
      {
        "libraryName": "mint-ui",
        "style": true
      }
    ]]
  ]
}